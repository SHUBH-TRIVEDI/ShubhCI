@using CI_Entities1.Models.ViewModel
@model AdminVM

<div class="fs-4">
    Skills
</div>

<div class="row">
    <hr class="col-1" style="border: 2px solid black;">
    <hr class="col-12">
</div>

<table id="myTable" class="display">
    <thead>
        <tr>
            <th>Skill Name</th>
            <th>Status</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var skill in Model.skills)
        {
            <tr id="application">
                <td>@skill.SkillName</td>
                <td>@skill.Status</td>
            </tr>
        }
    </tbody>
</table>

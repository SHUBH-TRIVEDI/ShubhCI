@using CI_Entities1.Models.ViewModel
@model AdminVM

<div class="fs-4">
    Mission Theme
</div>

<div class="row">
    <hr class="col-3" style="border: 2px solid black;">
    <hr class="col-9">
</div>

<table id="myTable" class="display">
    <thead>
        <tr>
            <th>Theme Name</th>
            <th>Status</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var theme in Model.missionThemes)
        {
            <tr id="application">
                <td>@theme.Title</td>
                <td>@theme.Status</td>
            </tr>
        }
    </tbody>
</table>